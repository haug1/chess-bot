{
  "name": "chess-bot-backend",
  "version": "1.0.0",
  "description": "Simple stockfish HTTP server",
  "main": "src/main.js",
  "scripts": {
    "start": "node --experimental-wasm-threads --experimental-wasm-simd src/main.js",
    "test": "node test.js",
    "test:http": "bash test.sh",
    "postinstall": "cp node_modules/stockfish/src/stockfish.js node_modules/stockfish/src/stockfish.worker.js"
  },
  "dependencies": {
    "express": "^4.18.1",
    "stockfish": "^15.0.0"
  },
  "keywords": [
    "stockfish",
    "nodejs"
  ]
}
